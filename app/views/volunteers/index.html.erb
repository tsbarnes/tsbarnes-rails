<p id="notice"><%= notice %></p>

<h1>Volunteers</h1>

<table>
  <thead>
    <tr>
      <th>Resume</th>
      <th>Organization</th>
      <th>Position</th>
      <th>Website</th>
      <th>Start date</th>
      <th>End date</th>
      <th>Summary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @volunteers.each do |volunteer| %>
      <tr>
        <td><%= volunteer.resume_id %></td>
        <td><%= volunteer.organization %></td>
        <td><%= volunteer.position %></td>
        <td><%= volunteer.website %></td>
        <td><%= volunteer.start_date %></td>
        <td><%= volunteer.end_date %></td>
        <td><%= volunteer.summary %></td>
        <td><%= link_to 'Show', volunteer %></td>
        <td><%= link_to 'Edit', edit_volunteer_path(volunteer) %></td>
        <td><%= link_to 'Destroy', volunteer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Volunteer', new_volunteer_path %>

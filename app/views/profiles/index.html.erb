<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Resume</th>
      <th>Network</th>
      <th>Icon name</th>
      <th>Username</th>
      <th>Url</th>
      <th>Order</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.resume_id %></td>
        <td><%= profile.network %></td>
        <td><%= profile.icon_name %></td>
        <td><%= profile.username %></td>
        <td><%= profile.url %></td>
        <td><%= profile.order %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
